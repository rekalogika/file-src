{
    "name": "rekalogika/file-symfony-bridge",
    "description": "Provides integrations for Rekalogika FileInterface with Symfony HttpFoundation, Form, and Validator.",
    "homepage": "https://rekalogika.dev/file-bundle",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "file",
        "symfony",
        "httpfoundation",
        "form",
        "validator",
        "adapter",
        "response",
        "streaming"
    ],
    "authors": [
        {
            "name": "Priyadi Iman Nurcahyo",
            "email": "priyadi@rekalogika.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Rekalogika\\File\\Bridge\\Symfony\\": "src/"
        }
    },
    "require": {
        "php": "^8.2",
        "rekalogika/file": "^1.14",
        "rekalogika/file-contracts": "^1.14",
        "rekalogika/file-metadata-contracts": "^1.14",
        "rekalogika/file-metadata": "^1.14",
        "symfony/form": "^6.2 || ^7.0",
        "symfony/http-foundation": "^6.2 || ^7.0",
        "symfony/validator": "^6.3 || ^7.1"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.15-dev"
        }
    }
}
