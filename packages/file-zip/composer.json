{
    "name": "rekalogika/file-zip",
    "description": "Streams a collection of files as a ZIP file to the client",
    "homepage": "https://rekalogika.dev/file-bundle",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "file",
        "symfony",
        "zipstream",
        "zip",
        "archive",
        "download",
        "stream",
        "http"
    ],
    "authors": [
        {
            "name": "Priyadi Iman Nurcahyo",
            "email": "priyadi@rekalogika.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Rekalogika\\File\\Zip\\": "src/"
        }
    },
    "require": {
        "psr/http-message": "^1.0 || ^2.0",
        "rekalogika/file-contracts": "^1.8.8",
        "rekalogika/file-metadata": "^1.8.8",
        "rekalogika/temporary-url-bundle": "^1.3",
        "maennchen/zipstream-php": "^3.1",
        "symfony/http-foundation": "^6.2 || ^7.0",
        "symfony/translation-contracts": "^3.0"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.9-dev"
        }
    }
}
