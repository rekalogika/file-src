{
    "name": "@rekalogika/file-filepond",
    "description": "Integrates FilePond with rekalogika/file framework",
    "license": "MIT",
    "version": "1.6.0",
    "symfony": {
        "controllers": {
            "filepond": {
                "main": "dist/filepond.js",
                "webpackMode": "eager",
                "fetch": "eager",
                "enabled": true,
                "autoimport": {}
            }
        },
        "importmap": {
            "@hotwired/stimulus": "^3.0"
        }
    },
    "scripts": {
        "build": "babel src -d dist"
    },
    "peerDependencies": {
        "@hotwired/stimulus": "^3.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.25",
        "@babel/core": "^7.25",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/preset-env": "^7.25.4",
        "@hotwired/stimulus": "^3.0"
    },
    "dependencies": {
        "filepond": "^4.31.3",
        "filepond-plugin-file-encode": "^2.1.11",
        "filepond-plugin-file-metadata": "^1.0.8",
        "filepond-plugin-file-poster": "^2.5.1",
        "filepond-plugin-file-validate-size": "^2.2.8",
        "filepond-plugin-file-validate-type": "^1.2.8",
        "filepond-plugin-image-crop": "^2.0.6",
        "filepond-plugin-image-edit": "^1.6.3",
        "filepond-plugin-image-exif-orientation": "^1.0.11",
        "filepond-plugin-image-preview": "^4.6.11",
        "filepond-plugin-image-resize": "^2.0.10",
        "filepond-plugin-image-transform": "^3.8.7",
        "filepond-plugin-image-validate-size": "^1.2.7"
    }
}
