{
    "name": "rekalogika/file-bundle",
    "description": "Integrates the rekalogika/file library into the Symfony framework.",
    "homepage": "https://rekalogika.dev/file-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Priyadi Iman Nurcahyo",
            "email": "priyadi@rekalogika.com"
        }
    ],
    "keywords": [
        "file",
        "filesystem",
        "flysystem",
        "symfony",
        "symfony-bundle",
        "upload",
        "file-upload"
    ],
    "autoload": {
        "psr-4": {
            "Rekalogika\\File\\Bundle\\": "src/"
        }
    },
    "require": {
        "league/flysystem": "^3.16",
        "league/flysystem-local": "^3.16",
        "psr/container": "^2.0",
        "rekalogika/file": "^1.8.7",
        "rekalogika/file-contracts": "^1.8.7",
        "symfony/config": "^6.2 || ^7.0",
        "symfony/dependency-injection": "^6.2 || ^7.0",
        "symfony/http-kernel": "^6.2 || ^7.0"
    },
    "require-dev": {
        "symfony/routing": "^6.2 || ^7.0"
    },
    "suggest": {
        "rekalogika/file-image": "Allows easy resizing image",
        "rekalogika/file-association": "Attach files to entities",
        "rekalogika/file-server": "Temporary URL generator for FileInterface",
        "rekalogika/file-symfony-bridge": "Adapters for Symfony components",
        "rekalogika/file-zip": "Stream files to web browsers as a ZIP archive"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.9-dev"
        }
    }
}
