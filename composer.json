{
    "name": "rekalogika/file-src",
    "description": "Monorepo for rekalogika/file and related packages",
    "homepage": "https://rekalogika.dev/file",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Priyadi Iman Nurcahyo",
            "email": "priyadi@rekalogika.com"
        }
    ],
    "require": {
        "cardinalby/content-disposition": "^1.1",
        "doctrine/collections": "^2.0",
        "doctrine/persistence": "^3.1",
        "fileeye/mimemap": "^2.0",
        "intervention/image": "^2.5",
        "league/flysystem": "^3.16",
        "league/flysystem-local": "^3.16",
        "league/mime-type-detection": "^1.0.0",
        "maennchen/zipstream-php": "^3.1",
        "nyholm/psr7": "^1.8",
        "oneup/uploader-bundle": "^4.0",
        "php": "^8.1",
        "php-http/discovery": "^1.15",
        "psr/container": "^2.0",
        "psr/http-factory-implementation": "^1.0",
        "psr/http-message": "^1.0 || ^2.0",
        "rekalogika/direct-property-access": "^1.1.2 || ^1.2",
        "rekalogika/doctrine-collections-decorator": "^2.0",
        "rekalogika/reconstitutor": "^1.0",
        "rekalogika/temporary-url-bundle": "^1.3",
        "symfony/config": "^6.2 || ^7.0",
        "symfony/dependency-injection": "^6.2 || ^7.0",
        "symfony/deprecation-contracts": "^3.1",
        "symfony/form": "^6.2 || ^7.0",
        "symfony/http-foundation": "^6.2 || ^7.0",
        "symfony/http-kernel": "^6.2 || ^7.0",
        "symfony/options-resolver": "^6.2 || ^7.0",
        "symfony/property-access": "^6.2 || ^7.0",
        "symfony/service-contracts": "^3.3",
        "symfony/stimulus-bundle": "^2.12",
        "symfony/translation-contracts": "^3.0",
        "symfony/validator": "^6.2 || ^7.0",
        "twig/twig": "^2.12|^3.0",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "bnf/phpstan-psr-container": "^1.0",
        "doctrine/doctrine-bundle": "^2.10",
        "ekino/phpstan-banned-code": "^1.0",
        "league/flysystem-memory": "^3.16",
        "mockery/mockery": "^1.6",
        "phpstan/phpstan": "^1.10.50",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-mockery": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpunit/phpunit": "^9.6",
        "psalm/plugin-mockery": "^1.1",
        "psalm/plugin-phpunit": "^0.18.4",
        "symfony/framework-bundle": "^6.2 || ^7.0",
        "symfony/phpunit-bridge": "^6.2 || ^7.0",
        "symfony/routing": "^6.2 || ^7.0",
        "symfony/yaml": "^6.2 || ^7.0",
        "symplify/monorepo-builder": "^11.2",
        "vimeo/psalm": "^5.18"
    },
    "autoload": {
        "psr-4": {
            "Rekalogika\\Contracts\\File\\": "packages/file-contracts/src/",
            "Rekalogika\\Contracts\\File\\Association\\": "packages/file-association-contracts/src/",
            "Rekalogika\\Contracts\\File\\Metadata\\": "packages/file-metadata-contracts/src/",
            "Rekalogika\\Contracts\\File\\Tree\\": "packages/file-tree-contracts/src/",
            "Rekalogika\\Domain\\File\\Association\\Entity\\": "packages/file-association-entity/src/",
            "Rekalogika\\Domain\\File\\Metadata\\": "packages/file-metadata/src/",
            "Rekalogika\\Domain\\File\\Null\\": "packages/file-null/src/",
            "Rekalogika\\File\\": "packages/file/src/",
            "Rekalogika\\File\\Association\\": "packages/file-association/src/",
            "Rekalogika\\File\\Bridge\\FilePond\\": "packages/file-filepond/src/",
            "Rekalogika\\File\\Bridge\\Flysystem\\": "packages/file-flysystem-bridge/src/",
            "Rekalogika\\File\\Bridge\\OneupUploader\\": "packages/file-oneup-uploader-bridge/src/",
            "Rekalogika\\File\\Bridge\\Symfony\\": "packages/file-symfony-bridge/src/",
            "Rekalogika\\File\\Bundle\\": "packages/file-bundle/src/",
            "Rekalogika\\File\\Derivation\\": "packages/file-derivation/src/",
            "Rekalogika\\File\\Image\\": "packages/file-image/src/",
            "Rekalogika\\File\\Server\\": "packages/file-server/src/",
            "Rekalogika\\File\\Zip\\": "packages/file-zip/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rekalogika\\File\\Tests\\": "tests/"
        }
    },
    "replace": {
        "rekalogika/file": "1.8.0",
        "rekalogika/file-association": "1.8.0",
        "rekalogika/file-association-contracts": "1.8.0",
        "rekalogika/file-association-entity": "1.8.0",
        "rekalogika/file-bundle": "1.8.0",
        "rekalogika/file-contracts": "1.8.0",
        "rekalogika/file-derivation": "1.8.0",
        "rekalogika/file-filepond": "1.8.0",
        "rekalogika/file-image": "1.8.0",
        "rekalogika/file-metadata": "1.8.0",
        "rekalogika/file-metadata-contracts": "1.8.0",
        "rekalogika/file-null": "1.8.0",
        "rekalogika/file-oneup-uploader-bridge": "1.8.0",
        "rekalogika/file-server": "1.8.0",
        "rekalogika/file-symfony-bridge": "1.8.0",
        "rekalogika/file-tree-contracts": "1.8.0",
        "rekalogika/file-zip": "1.8.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
